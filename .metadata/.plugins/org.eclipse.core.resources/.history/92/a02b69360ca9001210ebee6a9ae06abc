package kilobolt;

import java.awt.Image;

public class Tile {

	private int tileX, tileY, speedX, type;
	public Image tileImage;
	private Background bg = StartingClass.getBg1();
	
	public Tile(int tileX, int tileY, int type) {
		super();
		this.tileX = tileX;
		this.tileY = tileY;
		this.type = type;
		
		if (type == 1){
			tileImage = StartingClass.tileOcean;
		}
	}
	
	
	
}
